  <script>
    /**
     * ===========================
     * CONFIG (edite aqui)
     * ===========================
     */
    const CONFIG = {
      checkoutUrl: "https://SEU-LINK-DE-CHECKOUT-AQUI",
      whatsappNumber: "55DDDN√öMEROAQUI", // ex.: 5511999999999
      instagramUrl: "https://instagram.com/eliane_ptomas",
      // Timer opcional (bom para campanhas). Se n√£o quiser, coloque showTimer:false
      showTimer: true,
      // Ex.: "2026-02-05T23:59:59-03:00"  (hor√°rio do Brasil)
      offerEndsAt: "2026-12-31T23:59:59-03:00",
      privacyUrl: "#",
      termsUrl: "#",
    };

    const qs = (s)=>document.querySelector(s);

    function buildWhatsLink(){
      const msg = encodeURIComponent("Ol√°, quero entrar no M√©todo PAR. Pode me orientar?");
      const num = (CONFIG.whatsappNumber || "").replace(/\D/g,"");
      if(!num || num.length < 10) return "#";
      return `https://wa.me/${num}?text=${msg}`;
    }

    function wireLinks(){
      const whats = buildWhatsLink();

      // Checkout buttons
      ["#checkoutBtn","#stickyCheckout"].forEach(id=>{
        const el = qs(id);
        if(el) el.href = CONFIG.checkoutUrl;
      });

      // Whats buttons
      ["#heroWhats","#whatsBtn","#aboutWhats","#stickyWhats"].forEach(id=>{
        const el = qs(id);
        if(el) el.href = whats;
      });

      // Instagram
      const ig = qs("#igLink");
      if(ig) ig.href = CONFIG.instagramUrl;

      // Footer policies
      const p = qs("#privacy");
      const t = qs("#terms");
      if(p) p.href = CONFIG.privacyUrl;
      if(t) t.href = CONFIG.termsUrl;
    }

    function smoothAnchors(){
      document.addEventListener("click", (e)=>{
        const a = e.target.closest("a[href^='#']");
        if(!a) return;
        const id = a.getAttribute("href");
        if(id.length < 2) return;
        const target = document.querySelector(id);
        if(!target) return;
        e.preventDefault();
        target.scrollIntoView({behavior:"smooth", block:"start"});
      });
    }

    function pad2(n){ return String(n).padStart(2,"0"); }

    function startTimer(){
      const timer = qs("#timer");
      if(!timer) return;

      if(!CONFIG.showTimer){
        timer.classList.remove("on");
        return;
      }

      timer.classList.add("on");
      const end = new Date(CONFIG.offerEndsAt).getTime();

      function tick(){
        const now = Date.now();
        let diff = Math.max(0, end - now);

        const days = Math.floor(diff / (1000*60*60*24));
        diff -= days * (1000*60*60*24);
        const hrs  = Math.floor(diff / (1000*60*60));
        diff -= hrs * (1000*60*60);
        const mins = Math.floor(diff / (1000*60));
        diff -= mins * (1000*60);
        const secs = Math.floor(diff / 1000);

        qs("#d").textContent = pad2(days);
        qs("#h").textContent = pad2(hrs);
        qs("#m").textContent = pad2(mins);
        qs("#s").textContent = pad2(secs);

        if(end <= now){
          // quando acabar, voc√™ pode trocar o CTA/mostrar aviso.
          // Ex.: timer.classList.remove("on");
        }
      }

      tick();
      setInterval(tick, 1000);
    }

    function init(){
      wireLinks();
      smoothAnchors();
      startTimer();
      qs("#year").textContent = new Date().getFullYear();
    }
    init();
  </script>
